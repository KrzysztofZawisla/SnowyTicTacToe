{
  "name": "snowytictactoe",
  "version": "1.0.0",
  "description": "SnowyTicTacToe",
  "main": "app.js",
  "scripts": {
    "electron": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win-64": "electron-packager . --overwrite --asar=true --platform=win32 --arch=x64 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --icon=Img/tictactoe.ico --version-string.ProductName=\"SnowyTicTacToe\"",
    "package-win-32": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --icon=Img/tictactoe.ico --version-string.ProductName=\"SnowyTicTacToe\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrzysztofZawisla/SnowyTicTacToe.git"
  },
  "keywords": [
    "SnowyTicTacToe",
    "Krzysztof",
    "Zawisła",
    "Jakub",
    "Pawelec"
  ],
  "author": "Krzysztof Zawisła, Jakub Pawelec",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KrzysztofZawisla/SnowyTicTacToe/issues"
  },
  "homepage": "https://github.com/KrzysztofZawisla/SnowyTicTacToe#readme",
  "dependencies": {
    "electron-log": "^3.0.1",
    "update-electron-app": "^1.3.0"
  },
  "devDependencies": {
    "electron": "^4.0.8",
    "electron-packager": "^13.1.1"
  }
}
